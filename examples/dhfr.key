parameters      ../params/amoebapro
verbose
neighbor-list
a-axis                     62.23
vdw-cutoff                   9.0
ewald
ewald-cutoff                 7.0
ewald-alpha                  0.5446
pme-spacing                  1.0
pme-grid 64 64 64
pme-order                      5
polar-eps                   1.0
polarizeterm none

spacegroup P1
scfcache false


